{
  "name": "signing-authority-web",
  "version": "1.0.0",
  "description": "Vue.js based web application for SAA",
  "license": "ISC",
  "author": "Michael Johnson",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
